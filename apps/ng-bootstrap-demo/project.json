{
  "name": "ng-bootstrap-demo",
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "{projectRoot}/src",
  "prefix": "demo",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/{projectRoot}/browser",
        "index": "{projectRoot}/src/index.html",
        "main": "{projectRoot}/src/main.ts",
        "polyfills": "{projectRoot}/src/polyfills.ts",
        "tsConfig": "{projectRoot}/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "{projectRoot}/src/favicon.ico",
          "{projectRoot}/src/assets",
          "{projectRoot}/src/web.config"
        ],
        "styles": [
          "{projectRoot}/src/styles.scss",
          {
            "input": "libs/mintplayer-ng-bootstrap/src/styles/ngx-highlight-themes/solarized-dark.scss",
            "inject": false,
            "bundleName": "solarized-dark"
          }
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "5kb",
              "maximumError": "100kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "{projectRoot}/src/environments/environment.ts",
              "with": "{projectRoot}/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "ng-bootstrap-demo:build:production"
        },
        "development": {
          "buildTarget": "ng-bootstrap-demo:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "ng-bootstrap-demo:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "{projectRoot}/src/**/*.ts",
          "{projectRoot}/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "codeCoverage": true,
        "coverageReporters": ["lcovonly"],
        "coverageDirectory": "coverage/{projectRoot}",
        "jestConfig": "{projectRoot}/jest.config.ts"
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/ng-bootstrap-demo/server",
        "main": "{projectRoot}/server.ts",
        "tsConfig": "{projectRoot}/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "{projectRoot}/src/environments/environment.ts",
              "with": "{projectRoot}/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true,
          "buildOptimizer": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "ng-bootstrap-demo:build:development",
          "serverTarget": "ng-bootstrap-demo:server:development"
        },
        "production": {
          "browserTarget": "ng-bootstrap-demo:build:production",
          "serverTarget": "ng-bootstrap-demo:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "production": {
          "browserTarget": "ng-bootstrap-demo:build:production",
          "serverTarget": "ng-bootstrap-demo:server:production"
        },
        "development": {
          "browserTarget": "ng-bootstrap-demo:build:development",
          "serverTarget": "ng-bootstrap-demo:server:development"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "tags": []
}
