{
  "name": "mintplayer-bootstrap",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/mintplayer-bootstrap/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project mintplayer-bootstrap --web",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sass libs/mintplayer-bootstrap/src:dist/libs/mintplayer-bootstrap --no-source-map --style=compressed"
      }
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sass --watch libs/mintplayer-bootstrap/src:dist/libs/mintplayer-bootstrap --no-source-map"
      }
    }
  }
}
