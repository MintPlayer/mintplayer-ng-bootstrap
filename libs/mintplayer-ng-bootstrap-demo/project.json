{
  "name": "mintplayer-ng-bootstrap-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/mintplayer-ng-bootstrap-demo/src",
  "prefix": "mintplayer-ng-bootstrap",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/mintplayer-ng-bootstrap-demo/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mintplayer-ng-bootstrap-demo/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mintplayer-ng-bootstrap-demo/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/mintplayer-ng-bootstrap-demo/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/mintplayer-ng-bootstrap-demo/**/*.ts",
          "libs/mintplayer-ng-bootstrap-demo/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
